{{ form_start(form) }}

<div class="row">
    <div class="col-md-6">{{ form_row(form.nom) }}</div>
    <div class="col-md-6">{{ form_row(form.statut) }}</div>
</div>
<div class="row">
    <div class="col-md-6">{{ form_row(form.adresse) }}</div>
    <div class="col-md-6">{{ form_row(form.formation) }}</div>
</div>

<div class="row">
    <div class="col-md-3">{{ form_row(form.facebook) }}</div>
    <div class="col-md-3">{{ form_row(form.github) }}</div>
    <div class="col-md-3">{{ form_row(form.linkedIn) }}</div>
    <div class="col-md-3">{{ form_row(form.instagram) }}</div>
</div>

{{ form_rest(form) }}
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}